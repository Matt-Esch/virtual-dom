{
  "name": "virtual-dom",
  "version": "0.0.14",
  "description": "A batched diff-based DOM rendering strategy",
  "keywords": [],
  "author": "Matt-Esch <matt@mattesch.info>",
  "repository": "git://github.com/Matt-Esch/virtual-dom.git",
  "main": "index",
  "homepage": "https://github.com/Matt-Esch/virtual-dom",
  "contributors": [
    {
      "name": "Matt-Esch"
    }
  ],
  "bugs": {
    "url": "https://github.com/Matt-Esch/virtual-dom/issues",
    "email": "matt@mattesch.info"
  },
  "dependencies": {
    "browser-split": "0.0.1",
    "is-object": "0.1.2",
    "vdom": "0.0.14",
    "virtual-hyperscript": "^4.2.2",
    "vtree": "0.0.14",
    "x-is-array": "0.1.0",
    "x-is-string": "0.1.0"
  },
  "devDependencies": {
    "global": "^4.2.1",
    "min-document": "^2.6.1",
    "run-browser": "git://github.com/Raynos/run-browser",
    "tap-dot": "^0.2.1",
    "tap-spec": "^0.2.0",
    "tape": "^2.13.1"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/Matt-Esch/virtual-dom/raw/master/LICENSE"
    }
  ],
  "scripts": {
    "test": "node ./test/index.js | tap-spec",
    "dot": "node ./test/index.js | tap-dot",
    "start": "node ./index.js",
    "cover": "istanbul cover --report none --print detail ./test/index.js",
    "view-cover": "istanbul report html && google-chrome ./coverage/index.html",
    "browser": "run-browser test/index.js",
    "phantom": "run-browser test/index.js -b | tap-spec",
    "dist": "browserify --standalone virtual-dom index.js > dist/virtual-dom.js"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/8..latest",
      "firefox/17..latest",
      "firefox/nightly",
      "chrome/22..latest",
      "chrome/canary",
      "opera/12..latest",
      "opera/next",
      "safari/5.1..latest",
      "ipad/6.0..latest",
      "iphone/6.0..latest",
      "android-browser/4.2..latest"
    ]
  }
}
