  "dependencies": {
    "browser-split": "0.0.1",
    "cuid": "^1.2.1",
    "extend": "~1.2.1",
    "global": "git+https://github.com/Matt-Esch/global",
    "is-object": "^0.1.2",
    "x-is-array": "^0.1.0",
    "x-is-string": "^0.1.0"
  },
  "devDependencies": {
    "jshint": "^2.5.0",
    "min-document": "^2.3.0",
    "pre-commit": "0.0.5",
    "run-browser": "git://github.com/Raynos/run-browser",
    "tap-dot": "^0.2.1",
    "tap-spec": "^0.1.8",
    "tape": "^2.12.3"
